<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="objackie.dao.GongziFileDao">
  <resultMap type="gongziFile" id="gongziFileMap">
    <id column="PHO_NO" property="no"/> 
    <result column="BOD_NO" property="gongziNo"/>
    <result column="PHO_PATH" property="filename"/>
  </resultMap>
  
  <insert id="insert" parameterType="gongzi">
    insert into BOD_PHO (BOD_NO, PHO_PATH)
    values(#{gongziNo}, #{filename})
  </insert>
</mapper>


















